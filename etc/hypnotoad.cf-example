hypnotoad:
    proxy: 0
    listen: [ 'http://*:2794', 'https://*:2894' ]
    workers: 10
    accepts: 1000
    backlog: 255
    clients: 1000
    pid_file: 'run/haineko.pid'
    graceful_timeout: 15
    heartbeat_timeout: 15
    heartbeat_interval: 1
    inactivity_timeout: 60
    keep_alive_requests: 50

